package com.cts.railway;

import java.util.List;

public class Train {
	
	private int no;
	private String name;
	private char src;
	private char dest;
	private List<Seat> seats;
	private int cnf;
	private int wl;
	private int rac;
	
	public Train(int no, String name, char src, char dest, List<Seat> seats, int cnf, int wl, int rac) {
		super();
		this.no = no;
		this.name = name;
		this.src = src;
		this.dest = dest;
		this.seats = seats;
		this.cnf = cnf;
		this.wl = wl;
		this.rac = rac;
	}
	
	
	public int getCnf() {
		return cnf;
	}


	public void setCnf(int cnf) {
		this.cnf = cnf;
	}


	public int getWl() {
		return wl;
	}


	public void setWl(int wl) {
		this.wl = wl;
	}


	public int getRac() {
		return rac;
	}


	public void setRac(int rac) {
		this.rac = rac;
	}


	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public char getSrc() {
		return src;
	}
	public void setSrc(char src) {
		this.src = src;
	}
	public char getDest() {
		return dest;
	}
	public void setDest(char dest) {
		this.dest = dest;
	}
	public List<Seat> getSeats() {
		return seats;
	}
	public void setSeats(List<Seat> seats) {
		this.seats = seats;
	}
}
