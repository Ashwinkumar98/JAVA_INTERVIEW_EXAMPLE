package com.cts.toll;

public class PaymentDetails {
	
	private VehicelType type;
	private String No;
	private double amt;
	private String user;
	
	
	public PaymentDetails(VehicelType type, String no, double amt, String user) {
		super();
		this.type = type;
		No = no;
		this.amt = amt;
		this.user = user;
	}

	public String getUser() {
		return user;
	}

	public void setUser(String user) {
		this.user = user;
	}

	public VehicelType getType() {
		return type;
	}

	public void setType(VehicelType type) {
		this.type = type;
	}

	public String getNo() {
		return No;
	}

	public void setNo(String no) {
		No = no;
	}

	public double getAmt() {
		return amt;
	}

	public void setAmt(double amt) {
		this.amt = amt;
	}
	
}
